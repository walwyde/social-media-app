<%- include("./partials/nav.ejs") %>


<div class="container">

  <form class="tc" action="/register" method="post">
    <div class="sticker">
      <h3 class="centered">Welcome <i class="fas fa-user-friends fa-2x  "></i></h3>
    </div>
    <div class="form-group">
      <input type="text" name="user[username]"  placeholder="Enter username">
      <input type="email" name="user[email]"  placeholder="Enter Email">
      <input type="date" name="user[age]">
      <input type="password" name="password" placeholder="Enter Password">
    </div>
    <div class="form-group">
      <input type="submit" class="btn btn-secondary" value="Register">
    </div>
  </form>

</div>




